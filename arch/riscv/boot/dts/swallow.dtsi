			bus-width = <4>;
			pinctrl-names = "default";
			pinctrl-0 = <&mmc0_clk &mmc0_cmd &mmc0_bus4>;
			fifo-depth = <0x20>;
			cap-sd-highspeed;
			broken-cd;
			cd-inverted;
			nexell,drive_shift = <0x02>;
